{
  "version": "0.4.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/cstrnt/next-multipart",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "next": "^12.0.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "next-multipart",
  "author": "cstrnt",
  "module": "dist/next-multipart.esm.js",
  "size-limit": [
    {
      "path": "dist/next-multipart.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/next-multipart.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.5",
    "@types/formidable": "^2.0.3",
    "husky": "^7.0.4",
    "next": "^12.0.7",
    "node-mocks-http": "^1.11.0",
    "size-limit": "^7.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "formidable": "^2.0.1"
  }
